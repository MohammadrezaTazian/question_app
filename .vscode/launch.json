    "version": "0.2.0",
    "configurations": [
        
    
        {
            "name": "question_app (web server debug)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "debug",
            "args": [
                "-d",
                "edge",
                "web-server",
                "--web-hostname",
                "127.0.0.1",
                "--web-port",
                "9090"
            ]
        },
        {
            "name": "question_app (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "question_app (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}